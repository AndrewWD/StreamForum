    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.0",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.18.4",
    "react-hot-loader": "^4.3.7",
